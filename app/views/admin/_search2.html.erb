<%= javascript_include_tag :defaults %>
<h2>Search <%#= search_type %></h2>
<% form_tag :controller=>'admin',:action=>'search', :method => :get, :id => "frmSearch" do %>

    <%=text_field_tag :q, h(params[:q]), :id => "txtSearch", :class => "txt",
    :maxlength => 50 %>
    <%= submit_tag "Search", :name => nil, :id => "btnSearch",
    :class => "button" %>

<% end %>
